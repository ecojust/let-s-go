{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "let-s-go",
  "version": "0.1.0",
  "identifier": "com.let-s-go.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "  ",
        "transparent": true,
        "width": 900,
        "height": 600,
        "minWidth": 900,
        "minHeight": 600,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "android": {
      "minSdkVersion": 24,
      "versionCode": 1
    }
  },
  "plugins": {
    "dialog": {
      "all": true
    }
  }
}